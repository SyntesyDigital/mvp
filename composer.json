{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "acoustep/entrust-gui": "5.2.x-dev",
        "doctrine/dbal": "^2.7",
        "elasticsearch/elasticsearch": "^6.0",
        "facebook/graph-sdk": "^5.7",
        "fideloper/proxy": "~4.0",
        "fzaninotto/faker": "^1.8",
        "google/apiclient": "^2.2",
        "intervention/image": "^2.4",
        "jenssegers/date": "^3.3",
        "kalnoy/nestedset": "^4.3",
        "kevindierkx/laravel-domain-localization": "^2.0",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "~3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "league/oauth2-linkedin": "^4.1",
        "mariuzzo/laravel-js-localization": "^1.4",
        "mcamara/laravel-localization": "^1.3",
        "nwidart/laravel-modules": "^4.1",
        "petercoles/multilingual-country-list": "^1.2",
        "prettus/l5-repository": "^2.6",
        "syntesy-digital/insurance-installer": "dev-master",
        "tightenco/ziggy": "^0.6.6",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "guidocella/eloquent-populator": "^2.1",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
